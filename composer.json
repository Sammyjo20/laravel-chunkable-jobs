{
    "name": "sammyjo20/laravel-chunkable-jobs",
    "description": "Split a job into multiple separate job chunks",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sammyjo20\\ChunkableJobs\\": "src/",
            "Sammyjo20\\ChunkableJobs\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sammyjo20",
            "email": "29132017+Sammyjo20@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/bus": "^8.0|^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "pestphp/pest": "^1.21",
        "spatie/ray": "^1.33",
        "orchestra/testbench": "^7.7"
    },
    "scripts": {
        "test": [
            "./vendor/bin/pest"
        ],
        "fix-code": [
            "./vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
